<% image_class = '' if local_assigns[:image_class].nil? %>
<li class="partner-logos__listing">
  <%= link_to website, target: "_blank", rel: "noopener", title: t("shared.grayscale_partner_logo.provider_homepage", :provider_name => name) do %>
    <%= image_tag "partner-logos/grayscale/#{image_name}.svg", alt: name, class: "#{image_class}" %>
  <% end %>
</li>