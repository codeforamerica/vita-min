<div class="with-padding-med df-dependent-detail-form">
  <h2>Dependent Detail</h2>
  <%= f.state_file_nested_xml_field :DependentSSN %>
  <%= f.state_file_nested_xml_field :DependentFirstNm %>
  <%= f.state_file_nested_xml_field :DependentLastNm %>
  <div style="margin-bottom: 10px;">
    <%= f.label :DependentRelationshipCd, "<DependentRelationshipCd>" %>
    <div>
      <%= f.select(:DependentRelationshipCd, Efile::Relationship.all.map(&:irs_enum), include_blank: true) %>
    </div>
  </div>

  <%= f.hidden_field :_destroy %>
  <%= link_to_remove_fields t("views.shared.dependents_fields.remove"), '.df-dependent-detail-form', { class: "button button--small"} %>
</div>
