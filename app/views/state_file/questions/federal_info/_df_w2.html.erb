<div class="with-padding-med df-w2-form federal-info-controller-subform">
  <div class="federal-info-controller-subform-title">W2 <%= f.object.id %></div>
  <div class="federal-info-controller-subform-content">
    <%= render 'node_xml_reveal', xml: f.object.to_pretty_s %>

    <table>
      <%= f.state_file_nested_xml_field :WagesAmt %>
      <%= f.state_file_nested_xml_field :WithholdingAmt %>
      <%= f.state_file_nested_xml_field :StateWagesAmt %>
      <%= f.state_file_nested_xml_field :StateIncomeTaxAmt %>
      <%= f.state_file_nested_xml_field :LocalWagesAndTipsAmt %>
      <%= f.state_file_nested_xml_field :LocalIncomeTaxAmt %>
      <%= f.state_file_nested_xml_field :LocalityNm %>
    </table>

    <%= f.hidden_field :_destroy %>
    <%= link_to_remove_fields t("views.shared.dependents_fields.remove"), '.df-w2-form', { class: "button button--small"} %>
  </div>
</div>
