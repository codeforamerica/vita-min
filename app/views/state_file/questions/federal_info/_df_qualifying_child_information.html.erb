<div class="with-padding-med df-qualifying-child-information-form federal-info-controller-subform">
  <div class="federal-info-controller-subform-title">Qualifying Child Information</div>
  <div class="federal-info-controller-subform-content">
    <%= render 'node_xml_reveal', xml: f.object.to_pretty_s %>

    <table>
      <%= f.state_file_nested_xml_field :QualifyingChildSSN %>
      <tr>
        <td>
          <%= f.label :ChildRelationshipCd, "ChildRelationshipCd" %>
        </td>
        <td>
          <%= f.select(:ChildRelationshipCd, Efile::Relationship.all.map(&:irs_enum), include_blank: true) %>
        </td>
      </tr>
    </table>

    <%= f.hidden_field :_destroy %>
    <%= link_to_remove_fields t("views.shared.dependents_fields.remove"), '.df-qualifying-child-information-form', { class: "button button--small"} %>
  </div>
</div>
