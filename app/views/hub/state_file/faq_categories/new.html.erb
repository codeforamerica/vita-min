<% @title = "New FAQ Category" %>
<% content_for :page_title, @title %>
<% content_for :card do %>
  <div class="slab slab--not-padded spacing-above-25">
    <%= render 'hub/faq_categories/header', titles: [["Frequently Asked Questions", hub_state_file_faq_categories_path], @title] %>
    <hr/>
    <%= form_with model: @form, url: { product_type: params[:product_type], action: :create }, local: true, builder: VitaMinFormBuilder do |f| %>
      <%= render 'hub/faq_categories/faq_category_form', f: f, cancel_path: hub_state_file_faq_categories_path, edit_form: false, product_type: params[:product_type] %>
    <% end %>
  </div>
<% end %>